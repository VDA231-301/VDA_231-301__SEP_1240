    "Generic.ConsolidatedCharacteristicValue": {
      "allOf": [
        {
          "$ref": "#/$defs/Generic.InformationPoint"
        },
        {
          "properties": {
            "_id": {
              "$ref": "#/$defs/Generic.Identifier"
            },
            "_type": {
              "const": "ConsolidatedCharacteristicValue",
              "type": "string"
            },
            "Aggregation": {
              "$ref": "#/$defs/Generic.RestrictedString"
            },
            "Attachment": {
              "$ref": "#/$defs/Generic.Attachment"
            },
            "Comment": {
              "type": [
                "string",
