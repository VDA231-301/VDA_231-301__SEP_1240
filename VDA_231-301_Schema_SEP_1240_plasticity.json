        "TensileStrength": {
          "$ref": "#/$defs/SEP_1240_plasticity.TensileStrength"
        },
        "YieldStrength": {
          "$ref": "#/$defs/SEP_1240_plasticity.YieldStrength"
        },
        "ElongationA50": {
          "$ref": "#/$defs/SEP_1240_plasticity.ElongationA50"
        }
      },
      "required": [
        "_id",
        "_type",
        "Aggregation",
        "TensileStrength",
        "YieldStrength",
        "ElongationA50"
      ],
      "type": "object",
      "unevaluatedProperties": false
    },
